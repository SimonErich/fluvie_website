<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1920 1080">
  <defs>
    <linearGradient id="grid-bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#434343;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#000000;stop-opacity:1" />
    </linearGradient>
  </defs>

  <rect width="1920" height="1080" fill="url(#grid-bg)"/>

  <!-- Grid of images (3x3) -->
  <g transform="translate(510, 190)">
    <rect x="0" y="0" width="280" height="280" fill="#0175C2" stroke="white" stroke-width="4"/>
    <rect x="300" y="0" width="280" height="280" fill="#7C4DFF" stroke="white" stroke-width="4"/>
    <rect x="600" y="0" width="280" height="280" fill="#13B9FD" stroke="white" stroke-width="4"/>

    <rect x="0" y="300" width="280" height="280" fill="#FF006E" stroke="white" stroke-width="4"/>
    <rect x="300" y="300" width="280" height="280" fill="#8338EC" stroke="white" stroke-width="4"/>
    <rect x="600" y="300" width="280" height="280" fill="#00F5FF" stroke="white" stroke-width="4"/>
  </g>

  <text x="960" y="900" text-anchor="middle" font-family="Arial, sans-serif" font-size="54" font-weight="700" fill="white">
    The Grid Shuffle
  </text>

  <text x="960" y="960" text-anchor="middle" font-family="Arial, sans-serif" font-size="28" fill="rgba(255,255,255,0.7)">
    Photo Collage Template
  </text>
</svg>
